{
	"name": "td",
	"version": "0.1.0",
	"license": "ISC",
	"repository": "https://github.com/project-trio/td",
	"description": "Top-down 3D, multiplayer tower defense browser game.",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build --modern"
	},
	"devDependencies": {
		"@babel/core": "^7.15.0",
		"@babel/eslint-parser": "^7.15.0",
		"@fullhuman/postcss-purgecss": "^2.3.0",
		"@ky-is/eslint-config": "^1.9.3",
		"@ky-is/tailwind-color-palette": "^1.0.0",
		"@ky-is/tailwindcss-plugin-width-height": "^1.0.0",
		"@ky-is/trio-client": "^1.0.1",
		"@ky-is/vue-cli-plugin-tabs": "^1.1.0",
		"@ky-is/vue-cli-plugin-tailwind": "^2.0.0",
		"@vue/babel-preset-app": "^4.5.13",
		"@vue/cli-plugin-babel": "^4.5.13",
		"@vue/cli-plugin-eslint": "^4.5.13",
		"@vue/cli-service": "^4.5.13",
		"core-js": "^3.16.1",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^7.16.0",
		"postcss-preset-env": "^6.7.0",
		"seedrandom": "^3.0.5",
		"stats.js": "^0.17.0",
		"tailwindcss": "^1.9.6",
		"three": "^0.124.0",
		"three-voxel-loader": "^1.2.2",
		"typescript": "^4.3.5",
		"url-loader": "^4.1.1",
		"vue": "^2.6.14",
		"vue-router": "^3.5.2",
		"vue-template-compiler": "^2.6.14",
		"webpack": "^5.76.0"
	},
	"babel": {
		"presets": [
			[
				"@vue/app",
				{
					"jsx": false,
					"loose": true,
					"modules": "commonjs"
				}
			]
		]
	},
	"browserslist": {
		"production": [
			"> 1%",
			"last 2 versions",
			"not ie <= 8"
		],
		"development": [
			"last 1 version",
			"not dead"
		]
	},
	"eslintConfig": {
		"extends": "@ky-is/eslint-config/vue",
		"rules": {
			"vue/html-indent": [
				"warn",
				"tab",
				{
					"baseIndent": 0
				}
			]
		}
	}
}
